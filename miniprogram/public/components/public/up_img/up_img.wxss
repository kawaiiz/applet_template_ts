.img-block{width:135rpx;height:135rpx;margin:0 20rpx 20rpx 0;border-radius:10rpx}.img-block.item{position:relative;background-size:cover;background-repeat:no-repeat;background-position:center}.img-block.item .del{width:30rpx;height:30rpx;position:absolute;top:0;right:0;background-color:#677BEE;font-size:18rpx;line-height:30rpx;text-align:center;color:#fff;border-radius:50%}.img-block.item .error-tip{width:80rpx;height:80rpx;position:absolute;top:50%;left:50%;-webkit-transform:translate(-50%, -50%);transform:translate(-50%, -50%);line-height:80rpx;font-size:40rpx;border-radius:50%;background-color:red;color:#fff;text-align:center;z-index:1}.img-block.add{border:1rpx solid #D9D9D9;color:#D9D9D9;font-weight:lighter}.img-block.add .text{font-size:100rpx}
